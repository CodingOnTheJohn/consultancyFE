# See https://help.github.com/articles/ignoring-files/ for more about ignoring files.

# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
db/*.sqlite3
db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
log/*.log
tmp/**/*
tmp/pids/*
tmp/cache/*
tmp/sockets/*
!log/.keep
!tmp/.keep

# Ignore uploaded files in development.
storage/*
!storage/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore database schema dumps
/db/schema.rb
/db/structure.sql

# Ignore sensitive files
/config/credentials.yml.enc

# Ignore the .byebug history file.
/.byebug_history

# Ignore node_modules
/node_modules

# Ignore yarn error log
yarn-error.log

# Ignore coverage directory.
coverage/

# Ignore IDE/editor specific files
/.idea
.vscode/
*.swp
*~
.project
.idea/
.secret
.c9/
*.launch
.settings/
*.sublime-workspace

# Ignore system files
.DS_Store
Thumbs.db

# Ignore dotenv environment variable files
.env
.env.*

# Ignore additional config files
/config/database.yml
coverage