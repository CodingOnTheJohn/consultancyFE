<% if current_user %>
  Logged in as: <%= current_user %><br>
  <%= link_to "Sign In", login_path %><br>
  <%= link_to "Log Out", user_logout_path([:user_id]), method: :delete %><br>
<% else %>
  <%= link_to "Sign Up", new_user_path %>
<% end %>

<%= link_to "Login with Github",
"https://github.com/login/oauth/authorize?client_id=#{Rails.application.credentials.github[:client_id]}&scope=repo", data: { turbo: false } %>
